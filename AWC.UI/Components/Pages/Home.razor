					<!--<div class="hero-img col-sm-5 col-md-5" style="margin-left:-170px;margin-top:-80px;">
						<img src="assets/images/auth-bg.jpg" alt="image">
					</div>-->
					<div class="typing-container">
						<span id="typed-text" class="typing-text"></span><span class="cursor">|</span>
					</div>


					<script>
						document.addEventListener("DOMContentLoaded", () => {
							const text = "Welcome to Army War College, Mhow";
							const typedText = document.getElementById("typed-text");
							let index = 0;
							let isDeleting = false;

							function typeLoop() {
								if (!isDeleting) {
									typedText.textContent = text.substring(0, index++);
									if (index <= text.length) {
										setTimeout(typeLoop, 100);
									} else {
										isDeleting = true;
										setTimeout(typeLoop, 2000); // pause before deleting
									}
								} else {
									typedText.textContent = text.substring(0, --index);
									if (index >= 0) {
										setTimeout(typeLoop, 50);
									} else {
										isDeleting = false;
										setTimeout(typeLoop, 500);
									}
								}
							}

							typeLoop();
						});
					</script>


				</div>
			</div>
		</div>
